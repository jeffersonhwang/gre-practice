{{#unless isSummary}}

<!-- Displays a different view depending on question type-->
{{component model.questionType problem=model changed="setAnswer"}}

<div class="row">
  <div class="pull-left">
    <button type="submit" class="btn btn-primary" {{action "endPractice"}}>End Practice</button>
  </div>
  <div class="pull-right">
    <button type="submit" class="btn btn-default {{if (eq curProblemIndex problems.length) disabled ""}}" {{action "submitAnswer"}}>Submit Answer</button>
  </div>
</div>
{{/unless}}

{{#if isSummary}}
{{outlet}}
{{/if}}
